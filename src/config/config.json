{
    "dataset_path": "/mnt/d/Mestrado/MO839/data",
    "metadata_path": "/mnt/d/Mestrado/MO839/data/KidneyData.csv",
    "target_columns": ["target", "Class"],
    "save_path": "../../imgs",
    "group_column": "group",
    "num_workers": 0,
    "num_clients": 7,
    "random_seed": 42,
    "rounds": 22,
    "test_ratio": 0.15,
    "valid_ratio": 0.15,
    "Aggregation": "FedProx",
    "mu": 0.01,
    "data_transform": {
        "img_size": 224,
        "augmentations": {
            "RandomHorizontalFlip": {
                "p": 0.1
            },
            "RandomRotation": {
                "degrees": 8
            }
        },
        "normalizer": { 
            "mean":[0.485, 0.456, 0.406],
            "std":[0.229, 0.224, 0.225]
        },
        "heterogeneous_data_handler": {
            "augmentations": {
                "RandomRotation": {
                    "degrees": 7,
                    "fill": 0
                },
                "RandomAffine": {
                    "degrees": 0,
                    "translate": [0.04, 0.04],
                    "fill": 0
                },
                "RandomApply": {
                    "GaussianBlur": {
                        "kernel_size": 3
                    },
                    "p": 0.15
                },
                "RandomHorizontalFlip": {
                    "p": 0.1
                }
            }
        }
    },
    "model_config": {
        "model": "custom_layer4_fc_resnet18",
        "epochs": 3,
        "batch_size": 64,
        "learning_rate": 0.001,
        "criterion": {
            "CrossEntropyLoss": {"label_smoothing": 0.05}
        },
        "optimizer": "Adam",
        "weight_decay": 0.0001,
        "patience": 3,
        "lr_scheduler": {
            "StepLR": {
                "step_size": 2,
                "gamma": 0.5
            }
        }
    }
}